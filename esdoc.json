{
  "source": "./src",
  "destination": "./docs",
  "includes": ["Coverage.js", "CoverageCollection.js", "reader.js"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "/src/reader.js", "to": ""},
          {"from": "^src/", "to": "lib/"}
        ]
      }
    }
  ]
}