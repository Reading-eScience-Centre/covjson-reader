{
  "name": "covjson-reader",
  "version": "0.16.3",
  "license": "BSD-3-Clause",
  "description": "A reader for CoverageJSON files.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Reading-eScience-Centre/covjson-reader.git"
  },
  "bugs": {
    "url": "https://github.com/Reading-eScience-Centre/covjson-reader/issues"
  },
  "author": {
    "name": "Maik Riechert"
  },
  "main": "lib/reader.js",
  "esnext:main": "src/reader.js",
  "dependencies": {
    "covutils": "^0.6.0",
    "ndarray": "^1.0.18",
    "url-template": "^2.0.7"
  },
  "jspm": {
    "registry": "npm",
    "format": "esm",
    "main": "src/reader.js"
  },
  "files": [
    "src/",
    "lib/",
    "covjson-reader.*"
  ],
  "scripts": {
    "docs": "esdoc -c esdoc.json",
    "style": "standard",
    "transpile": "babel --presets es2015 -d lib/ src/",
    "test": "karma start --single-run",
    "karma": "karma start",
    "saucelabs": "karma start karma.conf-ci.js",
    "browserdist": "rollup -c rollup.config.src.js && rollup -c rollup.config.min.js",
    "build": "npm run transpile && npm run browserdist",
    "prepublish": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "karma.conf.js",
      "karma.conf-ci.js",
      "test/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^7.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "core-js": "^2.0.2",
    "esdoc": "^0.4.1",
    "esdoc-importpath-plugin": "0.0.1",
    "finalhandler": "^0.5.0",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-sauce-launcher": "^1.0.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "serve-static": "^1.10.0",
    "standard": "^8.4.0",
    "xndarray": "^0.3.0"
  }
}
