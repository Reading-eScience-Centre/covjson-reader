{
  "name": "covjson-reader",
  "version": "0.1.0",
  "main": "reader",
  "registry": "jspm",
  "format": "esm",
  "scripts": {
    "style": "standard",
    "test": "karma start --single-run",
    "karma": "karma start",
    "saucelabs": "karma start karma.conf-ci.js",
    "build": "jspm bundle-sfx covjson-reader/reader bundle.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "config.js",
      "karma.conf.js",
      "karma.conf-ci.js"
    ]
  },
  "jspm": {
    "directories": {
      "lib": "covjson-reader"
    },
    "dependencies": {
      "cbor": "github:paroga/cbor-js@^0.1.0",
      "ndarray": "npm:ndarray@^1.0.18"
    },
    "devDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.1.0",
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "jspm": "^0.16.1",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-sauce-launcher": "^0.2.14",
    "mocha": "^2.2.5",
    "standard": "^5.1.0"
  }
}
